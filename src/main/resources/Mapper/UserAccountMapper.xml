<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiesu.qingningbilling.mapper.UserAccountMapper">
  <resultMap id="baseResult" type="com.xiesu.qingningbilling.entity.UserAccount"
    extends="com.xiesu.qingningbilling.mapper.AbstractEntityMapper.AbstractResult">

    <result property="accountId" column="account_id"/>
    <result property="pwd" column="pwd"/>
    <result property="tel" column="tel"/>
    <result property="mail" column="mail"/>

    <result property="deleteFlag" column="delete_flag"/>
  </resultMap>
  <insert id="insertOne" parameterType="com.xiesu.qingningbilling.entity.UserAccount">
    insert into user_account
      (gmt_create, account_id, pwd, mail, tel)
    values (#{gmtCreate}, #{accountId}, #{pwd}, #{mail}, #{tel});
  </insert>

</mapper>
